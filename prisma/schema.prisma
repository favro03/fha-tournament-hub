generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Hotel {
  id      Int    @id @default(autoincrement())
  name    String
  address String
  phone   String
  image   String
  website String
}

model Restaurant {
  id          Int    @id @default(autoincrement())
  name        String
  address     String
  image       String
  website     String
  description String
}
